{
    "name": "chukshirley/laminas-ibmi-toolkit",
    "description": "Laminas MVC Module for the IBM i Toolkit",
    "type": "library",
    "authors": [
        {
            "name": "Chuk Shirley",
            "email": "chukShirley@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/chukShirley/laminas-ibmiToolkit"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "zendtech/ibmitoolkit": "^1.8.0",
        "laminas/laminas-mvc": "^3.2",
        "laminas/laminas-db": "^2.11"
    },
    "require-dev": {
        "ext-pdo": "*",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "LaminasIbmiToolkit\\": "src/"
        }
    },
  "autoload-dev": {
    "psr-4": {
      "LaminasIbmiToolkitIntegrationTest\\": "tests/LaminasIbmiToolkit/integration"
    }
  }
}
